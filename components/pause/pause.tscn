[sub_resource type="GDScript" id="GDScript_s77al"]
script/source = "extends MenuSelection

func _handle_select() -> void:
	var scene = get_parent().get_parent().get_parent()
	
	super()
	scene.offset.x -= 640
	await get_tree().physics_frame
	scene.get_node(\"Settings/Options\").focused = false
	scene.get_node(\"Controls/Options\").focused = true
"

[sub_resource type="GDScript" id="GDScript_jk6xi"]
script/source = "extends MenuSelection

func _handle_select() -> void:
	var scene = get_parent().get_parent().get_parent()
	
	super()
	scene.offset.y += 480
	await get_tree().physics_frame
	get_parent().move_selector(0)
	scene.get_node(get_parent().get_parent().main_menu_controls).focused = true
	scene.get_node(\"Settings/Options\").focused = false
	
	SettingsManager.save_settings()
"

